<wpml-config>
	<gutenberg-blocks>
	    <gutenberg-block type="gutencon/event" translate="1" label="Event box">
	        <key name="title" />
	        <key name="content" />
			<key name="location">
				<key name="name" />
				<key name="locality" />
				<key name="postal" />
				<key name="street" />
				<key name="region" />
				<key name="country" />
				<key name="stream" />
				<key name="maplabel" />
				<key name="streamlabel" />
			</key>
			<key name="offer">
				<key name="url" />
				<key name="price" />
				<key name="currency" />
			</key>
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/accordion" translate="1" label="Accordion">
			<key name="tabs">
				<key name="*">
					<key name="title" />
					<key name="content" />
				</key>
			</key>
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/tabs" translate="1" label="Tabs">
			<key name="tabTitles">
				<key name="*">
					<key name="title" />
					<key name="subtitle" />
				</key>
			</key>
			<xpath>//div[contains(@class, "tabtitlelabel")]</xpath>
			<xpath>//div[contains(@class, "tabsubtitle")]</xpath>
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/offerlisting" translate="1" label="Offer Listing">
			<key name="offers">
				<key name="*">
					<key name="title" />
					<key name="copy" />
					<key name="customBadge">
						<key name="text" />
					</key>
					<key name="currentPrice" />
					<key name="oldPrice" />
					<key name="button">
						<key name="text" />
					</key>
					<key name="couponCode" />
					<key name="read">
						<key name="readMore" />
					</key>
					<key name="disclaimer" />
				</key>
			</key>
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/offerlistingfull" translate="1" label="Listing builder">
			<key name="offers">
				<key name="*">
					<key name="title" />
					<key name="scoretext" />
					<key name="copy" />
					<key name="expandcontent" />
					<key name="customBadge">
						<key name="text" />
					</key>
					<key name="currentPrice" />
					<key name="oldPrice" />
					<key name="button">
						<key name="text" />
					</key>
					<key name="couponCode" />
					<key name="moretext" />
					<key name="disclaimer" />
				</key>
			</key>
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/linelist" translate="1" label="Connected list">
			<key name="items">
				<key name="*">
					<key name="content" />
				</key>
			</key>
			<xpath>//div[contains(@class, "gc-linelist-content")]/div</xpath>
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/howto" translate="1" label="Howto">
			<key name="title" />
			<key name="description" />
			<key name="tabs">
				<key name="*">
					<key name="title" />
					<key name="content" />
				</key>
			</key>
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/comparison-item" translate="1" label="Comparison item">
			<key name="productBadge" />		
			<key name="productTitle" />		
			<key name="productSubtitle" />		
			<key name="bottomText" />		
			<key name="prosText" />		
			<key name="consText" />		
			<key name="specText" />		
			<key name="buttonUrl" />		
			<key name="buttonText" />		
			<key name="listTitle" />				
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/comparison-table" translate="1" label="Comparison table">
			<key name="bottomTitle" />		
			<key name="prosTitle" />		
			<key name="consTitle" />		
			<key name="specTitle" />						
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/countdown" translate="1" label="Countdown">
			<key name="labelDay" />		
			<key name="labelHour" />		
			<key name="labelMin" />		
			<key name="labelSec" />		
			<xpath>(//div[contains(@class, "gc-countdown__item_label")])[1]</xpath>
			<xpath>(//div[contains(@class, "gc-countdown__item_label")])[2]</xpath>	
			<xpath>(//div[contains(@class, "gc-countdown__item_label")])[3]</xpath>	
			<xpath>(//div[contains(@class, "gc-countdown__item_label")])[4]</xpath>					
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/numberheading" translate="1" label="Numbered heading">
			<key name="title" />		
			<key name="subtitle" />							
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/proscons" translate="1" label="Pros Cons">
			<key name="prosTitle" />		
			<key name="consTitle" />
			<key name="positives">
				<key name="*">
					<key name="title" />
				</key>
			</key>	
			<key name="negatives">
				<key name="*">
					<key name="title" />
				</key>
			</key>
			<xpath>//div[contains(@class, "gc-prcs-box__title--pros")]/span[2]</xpath>
   			<xpath>//div[contains(@class, "gc-prcs-box__title--cons")]/span[2]</xpath>
   			<xpath>//li[contains(@class, "gcbox-pros__item")]/span</xpath>
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/reviewbox" translate="1" label="Review Box">
			<key name="prosTitle" />		
			<key name="consTitle" />
			<key name="scorelabel" />		
			<key name="title" />
			<key name="description" />		
			<key name="positives">
				<key name="*">
					<key name="title" />
				</key>
			</key>	
			<key name="negatives">
				<key name="*">
					<key name="title" />
				</key>
			</key>
			<key name="criterias">
				<key name="*">
					<key name="title" />
				</key>
			</key>							
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/scorebox" translate="1" label="Score Box">
			<key name="title" />		
			<key name="label" />
			<key name="prosTitle" />		
			<key name="consTitle" />		
			<key name="positives">
				<key name="*">
					<key name="title" />
				</key>
			</key>	
			<key name="negatives">
				<key name="*">
					<key name="title" />
				</key>
			</key>
			<key name="buttons">
				<key name="*">
					<key name="btntitle" />
				</key>
			</key>							
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/promobox" translate="1" label="Promobox">
			<key name="title" />		
			<key name="content" />							
			<key name="buttonText" />							
			<key name="buttonLink" />							
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/titlebox" translate="1" label="Title box">
			<key name="title" />								
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/versus" translate="1" label="Versus">
			<key name="heading" />								
			<key name="subheading" />	
			<key name="firstColumn">
				<key name="content" />
			</key>															
			<key name="secondColumn">
				<key name="content" />
			</key>															
			<key name="thirdColumn">
				<key name="content" />
			</key>															
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/offerbox" translate="1" label="Offer box">
			<key name="name" />		
			<key name="description" />							
			<key name="disclaimer" />		
			<key name="old_price" />							
			<key name="sale_price" />		
			<key name="coupon_code" />							
			<key name="button">
				<key name="url" />
				<key name="text" />
			</key>						
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/contenttoggler" translate="1" label="Content Toggler">
			<key name="openlabel" />		
			<key name="closelabel" />
			<xpath>//span[contains(@class, "gc-tgl-show")]</xpath>	
			<xpath>//span[contains(@class, "gc-tgl-hide")]</xpath>													
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/progressbar" translate="1" label="Progress Bar">
			<key name="title" />		
			<key name="label" />
			<xpath>//span[contains(@class, "gc-progressbar__title")]/span</xpath>	
			<xpath>//span[contains(@class, "gc-progressbar__label")]/span</xpath>													
	    </gutenberg-block>
	    <gutenberg-block type="gutencon/counter" translate="1" label="Counter">
			<key name="label" />
			<xpath>//div[contains(@class, "gc-counterlabel")]</xpath>
	    </gutenberg-block>
	</gutenberg-blocks>
</wpml-config>